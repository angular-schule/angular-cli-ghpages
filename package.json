{
  "name": "ngx-gh",
  "version": "0.0.1",
  "description": "Deployment from the Angular CLI to GitHub pages",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && json2ts deploy/schema.json > deploy/schema.d.ts && tsc && cp README.md builders.json collection.json package.json dist && cp deploy/schema.json dist/deploy",
    "test": "jest"
  },
  "schematics": "./collection.json",
  "builders": "./builders.json",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgechev/ng-deploy-gh-pages.git"
  },
  "keywords": [
    "angular",
    "cli",
    "angular-cli",
    "ghpages",
    "github",
    "pages",
    "deploy",
    "ng deploy",
    "ng-deploy"
  ],
  "author": "Minko Gechev <mgechev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mgechev/ng-deploy-gh-pages/issues"
  },
  "homepage": "https://github.com/mgechev/ng-deploy-gh-pages#readme",
  "devDependencies": {
    "@angular-devkit/architect": "^0.800.6",
    "@angular-devkit/core": "^8.0.6",
    "@angular-devkit/schematics": "^8.0.6",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "jest": "^24.8.0",
    "json-schema-to-typescript": "^6.1.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "peerDependencies": {
    "@angular-devkit/architect": ">=0.800.0",
    "@angular-devkit/core": ">=8.0.0",
    "@angular-devkit/schematics": ">=8.0.0"
  },
  "dependencies": {
    "gh-pages": "^2.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
